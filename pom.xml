<?xml version="1.0" encoding="UTF-8"?>
<project>
	<modelVersion>4.0.0</modelVersion>
	<groupId>se.swami.saml.metadata</groupId>
	<artifactId>saml-md-aggregator</artifactId>
	<version>1.0-SNAPSHOT</version>
	<name>SAML Metadata Aggregator</name>
	<description>SAML Metadata Aggregator</description>
	<url>http://devel.it.su.se</url>
	<packaging>war</packaging>
	<developers>
		<developer>
			<id>leifj</id>
			<name>Leif Johansson</name>
			<email>leifj@it.su.se</email>
			<organization>Stockholm university</organization>
			<timezone>MET</timezone>
		</developer>
	</developers>
	<licenses>
		<license>
			<name>BSD</name>
			<url>http://devel.it.su.se/static/LICENSE.txt</url>
			<distribution>repo</distribution>
			<comments>Stockholm university BSD License</comments>
		</license>
	</licenses>
	<scm>
		<connection>scm:svn:svn://anonsvn.it.su.se/saml-md-aggregator
		</connection>
		<developerConnection>scm:svn:svn+ssh://svn.it.su.se/afs/su.se/services/svn/saml-md-aggregator
		</developerConnection>
	</scm>
	<organization>
		<name>SWAMI</name>
		<url>http://www.swami.se</url>
	</organization>
	<build>
		<defaultGoal>install</defaultGoal>
			<plugins>
				<plugin>
					<artifactId>maven-compiler-plugin</artifactId>
					<configuration>
						<source>1.5</source>
						<target>1.5</target>
					</configuration>
				</plugin>
				<plugin>
					<groupId>org.mortbay.jetty</groupId>
					<artifactId>maven-jetty-plugin</artifactId>
					<version>6.1.15</version>
					<configuration>
						<contextPath>/</contextPath>
					</configuration>
				</plugin>
			</plugins>
	</build>
	<modules>
		<module>core</module>
		<module>web</module>
	</modules>

	<properties>
		<spring.framework.version>3.0.0.RC2</spring.framework.version>
		<org.slf4j.version>1.5.2</org.slf4j.version>
	</properties>

	<repositories>
		<repository>
			<id>su-mvn</id>
			<name>SU Maven2</name>
			<url>http://maven.it.su.se/it.su.se/maven2 </url>
		</repository>
		<repository>
			<id>neo4j-public-repository</id>
			<url>http://m2.neo4j.org</url>
		</repository>
		<!--
			repository> <id>dev.java.net</id>
			<url>http://download.java.net/maven/2</url> </repository
		-->
		<repository>
			<id>ibiblio mirror</id>
			<url>http://mirrors.ibiblio.org/pub/mirrors/maven2/</url>
		</repository>
		<repository>
			<id>Springframework milestone</id>
			<url>http://maven.springframework.org/milestone</url>
		</repository>
		<repository>
			<id>jboss</id>
			<url>http://repository.jboss.org/maven2</url>
		</repository>
		<!-- repository>
			<id>java.net</id>
			<url>https://maven-repository.dev.java.net/nonav/repository</url>
			<layout>legacy</layout>
		</repository -->
		<repository>
			<id>codehaus</id>
			<url>http://repository.codehaus.org</url>
		</repository>
		<repository>
			<id>atlassian</id>
			<url>http://repository.atlassian.com/maven2</url>
		</repository>
		<!--  repository>
			<id>maven2-repository.dev.java.net</id>
			<name>Java.net Repository for Maven</name>
			<url>http://download.java.net/maven/2/</url>
			<layout>default</layout>
		</repository -->
	</repositories>
	<pluginRepositories>
		<pluginRepository>
			<id>maven2-repository.dev.java.net</id>
			<url>http://download.java.net/maven/2</url>
		</pluginRepository>
		<pluginRepository>
			<id>maven-repository.dev.java.net</id>
			<name>Java.net Maven 1 Repository (legacy)</name>
			<url>http://download.java.net/maven/1</url>
			<layout>legacy</layout>
		</pluginRepository>
	</pluginRepositories>
	<dependencyManagement>
